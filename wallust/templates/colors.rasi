/* Rofi Colors generated by Wallust */

* {
    background:  {{background}};
    foreground:  {{foreground}};
    color1:      {{color1}};   /* Accent color */
    color2:      {{color2}};   /* e.g. red variant */
    color3:      {{color3}};   /* e.g. green variant */
    color4:      {{color4}};   /* e.g. yellow variant */
    color5:      {{color5}};   /* e.g. blue variant */
    color6:      {{color6}};   /* e.g. purple variant */
    color7:      {{color7}};   /* e.g. cyan variant */
}
